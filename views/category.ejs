<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Menu</title>
</head>
<body>
  <%- include('./partials/nav') %>
  <h1><%= categoryName %></h1>

  <% menuItems.forEach((item) => { %>
    <% if (item.rating === 5) { %>
        <h3><%= item.name %> ⭐️</h3>
      <% } else { %>
        <h3><%= item.name %></h3>
     <% } %>
    
    <p>Price: <%= item.price %> Rating: <%= item.rating %></p>
    <p><%= item.details %></p>
  <% }) %>  
  
 


</body>
</html>
